<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.vitor0liveira.barber">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.github.vitor0liveira.barber.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.vitor0liveira.barber.view.LoginActivity" />
        <activity android:name=".view.ResetActivity" />
        <activity android:name=".view.SingupActivity" />
        <activity android:name=".view.CadastroClienteActivity" />
        <activity android:name=".view.CadastroServicoActivity" />
        <activity android:name="com.github.vitor0liveira.barber.view.ListClienteActivity" />
        <activity android:name="com.github.vitor0liveira.barber.view.ListServicoActivity" />

        <provider
            android:name="com.github.vitor0liveira.barber.model.BarberProvider"
            android:authorities="com.github.vitor0liveira.barber"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>